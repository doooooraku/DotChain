{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
      "distribution": "store",  // ← ★ここを "internal" から "store" に変更！
      "ios": {
        "resourceClass": "m1-medium" // ローカルビルドなら無視されますが念のため
    }
  },
  "submit": {
    "production": {}
  }
}
